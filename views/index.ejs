<!DOCTYPE html>
<html>
  <% include head %>
  <body>
    <div class ="container">
      <div class ="row">
        <div class ="col col-12">
        <h1>Movies Now Playing</h1>
      </div>
      <!-- we have an array! -->
        <% parsedData.forEach((movie, index)=>{ %>
       
                      <div class "col col-3 poster">
                        <%= movie.title %>
                        <div class="col col-3 poster">
                          <img src=" <%= imageBaseUrl + movie.poster_path %>">
                        </div>
                        <% }) %>
                      </div>
        </div>
      </div>
    </div>
  </body>
</html>
